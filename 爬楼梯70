int climbStairs(int n) {
    int f[n+2];//为了避免定义f1 f2时溢出
    f[1]=1;f[2]=2;
    for(int i = 3 ; i <= n ; i++ ){
        f[i] = f[i-1] + f[i-2];
    }
    return f[n];
}

改进
int climbStairs(int n) {
    int p =0,q=0,t=1; //用三个数字代替数组
    for(int i = 1 ; i <= n ; i++ ){
        p=q;
        q=t;
        t = p+q;
    }
    return t;
}
